  "require": {
    "php": ">=7.0.0",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "mix/framework": "~2.0.0",
    "mix/console": "~2.0.0",
    "mix/log": "~2.0.0",
    "mix/http": "~2.0.0",
    "mix/http-session": "~2.0.0",
    "mix/http-message": "~2.0.0",
    "mix/http-server": "~2.0.0",
    "mix/http-daemon": "~2.0.0",
    "mix/websocket": "~2.0.0",
    "mix/websocket-server": "~2.0.0",
    "mix/websocket-daemon": "~2.0.0",
    "mix/tcp-session": "~2.0.0",
    "mix/tcp": "~2.0.0",
    "mix/tcp-server": "~2.0.0",
    "mix/tcp-daemon": "~2.0.0",
    "mix/udp": "~2.0.0",
    "mix/udp-server": "~2.0.0",
    "mix/udp-daemon": "~2.0.0",
    "mix/validate": "~2.0.0",
    "mix/concurrent": "~2.0.0",
    "mix/pool": "~2.0.0",
    "mix/database": "~2.0.0",
    "mix/redis": "~2.0.0",
    "mix/cache": "~2.0.0",
    "mix/auth": "~2.0.0"
  },
  "require-dev": {
    "swoole/ide-helper": "dev-master"
